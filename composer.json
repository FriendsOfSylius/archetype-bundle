{
    "name": "fosyl/archetype-bundle",
    "type": "symfony-bundle",
    "description": "Archetypes of models for your Sylius and Symfony2 applications.",
    "keywords": ["shop", "ecommerce", "products", "product", "assortment", "prototypes", "archetypes"],
    "homepage": "https://github.com/friendsofsylius",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Elsodaney",
            "homepage": "http://github.com/adamelso"
        },
        {
            "name": "Paweł Jędrzejewski",
            "homepage": "http://pjedrzejewski.com"
        },
        {
            "name": "Sylius project",
            "homepage": "http://sylius.org"
        },
        {
            "name": "Community contributions",
            "homepage": "http://github.com/friendsofsylius/archetype-bundle/contributors"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",

        "fosyl/archetype": "dev-master",
        "sylius/product": "^1.0 | ^0.19",
        "sylius/attribute-bundle": "^1.0 | ^0.19",
        "sylius/resource-bundle": "^1.0 | ^0.19",
        "symfony/framework-bundle": "^2.8"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.0",
        "symfony/form": "^2.8",
        "symfony/validator": "^2.8",
        "doctrine/orm": "^2.4.8,<2.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylius\\Bundle\\ArchetypeBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "spec\\Sylius\\Bundle\\ArchetypeBundle\\": "spec/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.20-dev"
        }
    }
}
